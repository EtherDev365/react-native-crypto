{
  "expo": {
    "name": "cfn-mobile-application",
    "slug": "cfn-mobile-application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_light_transparent.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "io.cenfura.cfn_mobile_application",
      "permissions": ["CAMERA", "INTERNET"]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
